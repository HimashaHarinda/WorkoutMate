extends layout

block content
  header.main-header
  nav.navbar.navbar-static-top#navbar
    .container
      .navbar-header
        a.navbar-brand
          b Workout
          | Mate.      
        button.navbar-toggle.collapsed(type="button", data-toggle="collapse", data-target="#navbar-collapse")
          i.fa.fa-bars
      // Collect the nav links, forms, and other content for toggling
      #navbar-collapse.collapse.navbar-collapse.pull-left
        ul.nav.navbar-nav          
              li#username
                a #{sessionUser}
              
              li
                a(href="/") Logout

  each users, user in dash  
    .info-box.bg-blue
            span.info-box-icon
                i.ion.ion-ios-person-outline
            .info-box-content
                span.info-box-number #{users.FullName} 
                span.info-box-text  #{users.userEmail}
                span.info-box-text  #{users.userType}
                a.btn.btn-info.pull-left(href="/deleteuser/#{users.userEmail}") Delete



          